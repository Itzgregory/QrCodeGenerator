{
  "name": "qr-code-server",
  "version": "1.0.0",
  "scripts": {
    "start": "nest start",
    "dev": "nest start --watch --tsc",
    "build": "nest build",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "@prisma/client": "^4.0.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "qrcode": "^1.5.0",
    "rxjs": "^7.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@types/express": "^5.0.3",
    "@types/node": "^16.0.0",
    "@types/qrcode": "^1.5.5",
    "prisma": "^4.0.0",
    "typescript": "^4.6.0"
  }
}
